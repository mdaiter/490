/**
 * 16 bit multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux16 {
    IN a[16], b[16], sel;
    OUT out[16];

    PARTS:
    // Put your code here:
    Not(in=sel, out=notSel);
    And16(a=b, b=sel, out=bSel);
    And16(a=a, b=notSel, out=aNotSel);

    Or16(a=aNotSel, b=bSel, out=out);
}

